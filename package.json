    "seed": "copy .env.dev .env && knex seed:run --env development",
    "dev": "copy .env.dev .env && node ./bin/www",
    "dev-migrate": "copy .env.dev .env && knex migrate:latest",
    "dev-rollback": "copy .env.dev .env && knex migrate:rollback",
    "dev-seed": "copy .env.dev .env && knex seed:run"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
